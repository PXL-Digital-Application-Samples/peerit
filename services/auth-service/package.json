{
  "name": "@peerit/auth-service",
  "version": "1.0.0",
  "private": true,
  "description": "Authentication and session management service for Peerit platform",
  "main": "src/index.js",
  "scripts": {
    "dev": "nodemon src/index.js",
    "start": "node src/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:integration": "jest --testMatch='**/tests/integration/**/*.test.js'",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "db:migrate": "prisma migrate dev",
    "db:migrate:new": "prisma migrate dev --create-only",
    "db:reset": "prisma migrate reset",
    "db:seed": "node src/scripts/seed.js",
    "db:generate": "prisma generate"
  },
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "prisma": "^5.22.0",
    "redis": "^4.6.10",
    "swagger-ui-express": "^5.0.0",
    "uuid": "^9.0.1",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.8",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "authentication",
    "jwt",
    "microservice",
    "express",
    "peerit"
  ]
}
