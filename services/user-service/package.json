{
  "name": "peerit-user-service-generated",
  "version": "1.0.0",
  "description": "User profiles, roles, and team membership management for the Peerit platform",
  "main": "index.js",
  "scripts": {
    "prestart": "npm install",
    "start": "node index.js",
    "dev": "nodemon index.js",
    "test": "mocha --recursive",
    "test:unit": "mocha tests/unit/ --recursive",
    "test:integration": "mocha tests/integration/ --recursive --timeout 10000",
    "lint": "eslint *.js controllers/ services/ middleware/ utils/",
    "lint:fix": "eslint *.js controllers/ services/ middleware/ utils/ --fix",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:migrate:new": "prisma migrate dev --create-only",
    "db:reset": "prisma migrate reset",
    "db:studio": "prisma studio",
    "docker:build": "docker build -t peerit-user-service-generated .",
    "docker:build:multi": "docker build -f Dockerfile.multi-stage -t peerit-user-service-generated:multi .",
    "docker:run": "docker run -p 3022:3020 --env-file .env peerit-user-service-generated",
    "compose:up": "docker compose up -d",
    "compose:down": "docker compose down",
    "compose:test:up": "docker compose -f compose.test.yml up -d",
    "compose:test:down": "docker compose -f compose.test.yml down --volumes"
  },
  "keywords": [
    "openapi-generator",
    "openapi"
  ],
  "license": "Unlicense",
  "private": true,
  "dependencies": {
    "@prisma/client": "^6.13.0",
    "axios": "^1.6.2",
    "body-parser": "^1.20.2",
    "camelcase": "^6.3.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-openapi-validator": "^5.1.4",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "js-yaml": "^4.1.0",
    "jsonwebtoken": "^9.0.2",
    "jwk-to-pem": "^2.0.7",
    "ono": "^7.1.3",
    "openapi-sampler": "^1.5.0",
    "swagger-ui-express": "^5.0.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "chai": "^4.4.1",
    "chai-as-promised": "^7.1.1",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.29.0",
    "mocha": "^10.2.0",
    "nodemon": "^3.0.0",
    "prisma": "^6.13.0"
  },
  "eslintConfig": {
    "env": {
      "node": true
    }
  },
  "prisma": {
    "seed": "node prisma/seed.js"
  }
}
